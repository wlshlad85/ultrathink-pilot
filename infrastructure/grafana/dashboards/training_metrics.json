          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "WITH stats AS (\n  SELECT\n    timestamp,\n    value,\n    AVG(value) OVER (ORDER BY timestamp ROWS BETWEEN 9 PRECEDING AND CURRENT ROW) as avg_10,\n    STDDEV(value) OVER (ORDER BY timestamp ROWS BETWEEN 9 PRECEDING AND CURRENT ROW) as std_10,\n    AVG(value) OVER (ORDER BY timestamp ROWS BETWEEN 49 PRECEDING AND CURRENT ROW) as avg_50,\n    STDDEV(value) OVER (ORDER BY timestamp ROWS BETWEEN 49 PRECEDING AND CURRENT ROW) as std_50,\n    AVG(value) OVER (ORDER BY timestamp ROWS BETWEEN 99 PRECEDING AND CURRENT ROW) as avg_100,\n    STDDEV(value) OVER (ORDER BY timestamp ROWS BETWEEN 99 PRECEDING AND CURRENT ROW) as std_100\n  FROM experiment_metrics\n  WHERE\n    metric_name = 'episode_return_pct'\n    AND experiment_id = $experiment_id\n    AND $__timeFilter(timestamp)\n)\nSELECT timestamp as time, CASE WHEN std_10 > 0 THEN avg_10 / std_10 ELSE 0 END as \"10-Episode Sharpe\" FROM stats\nUNION ALL\nSELECT timestamp as time, CASE WHEN std_50 > 0 THEN avg_50 / std_50 ELSE 0 END as \"50-Episode Sharpe\" FROM stats\nUNION ALL\nSELECT timestamp as time, CASE WHEN std_100 > 0 THEN avg_100 / std_100 ELSE 0 END as \"100-Episode Sharpe\" FROM stats\nORDER BY time",
          "refId": "A",
          "select": [
            [
              {
                "params": ["value"],
                "type": "column"
              }
            ]
          ],
          "timeColumn": "timestamp",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        }
      ],
      "title": "Rolling Sharpe Ratio (10/50/100 Episode Windows)",
      "type": "timeseries"
