          "rawSql": "SELECT\n  timestamp as time,\n  'Bull' as metric,\n  regime_prob_bull as \"Bull\"\nFROM regime_predictions\nWHERE\n  timestamp >= $__timeFrom()::timestamptz\n  AND timestamp <= $__timeTo()::timestamptz\nUNION ALL\nSELECT\n  timestamp as time,\n  'Bear' as metric,\n  regime_prob_bear as \"Bear\"\nFROM regime_predictions\nWHERE\n  timestamp >= $__timeFrom()::timestamptz\n  AND timestamp <= $__timeTo()::timestamptz\nUNION ALL\nSELECT\n  timestamp as time,\n  'Sideways' as metric,\n  regime_prob_sideways as \"Sideways\"\nFROM regime_predictions\nWHERE\n  timestamp >= $__timeFrom()::timestamptz\n  AND timestamp <= $__timeTo()::timestamptz\nORDER BY time ASC",
