        {
          "datasource": {
            "type": "postgres",
            "uid": "TimescaleDB"
          },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "WITH pnl_buckets AS (\n  SELECT\n    width_bucket(pnl_pct, -10, 10, 40) as bucket,\n    COUNT(*) as frequency,\n    experiment_id\n  FROM trade_decisions\n  WHERE\n    timestamp >= $__timeFrom()::timestamptz\n    AND timestamp <= $__timeTo()::timestamptz\n    AND ($experiment_id = '' OR experiment_id = $experiment_id)\n    AND pnl_pct IS NOT NULL\n  GROUP BY bucket, experiment_id\n)\nSELECT\n  NOW() as time,\n  CASE \n    WHEN ((bucket - 1) * 0.5 - 10) >= 0 THEN 'Profit: [' || ROUND(((bucket - 1) * 0.5 - 10)::numeric, 1) || '% to ' || ROUND((bucket * 0.5 - 10)::numeric, 1) || '%]'\n    ELSE 'Loss: [' || ROUND(((bucket - 1) * 0.5 - 10)::numeric, 1) || '% to ' || ROUND((bucket * 0.5 - 10)::numeric, 1) || '%]'\n  END as metric,\n  frequency as \"Frequency\"\nFROM pnl_buckets\nORDER BY bucket",
          "refId": "A"
        }
      ],
      "title": "P&L Per Trade (Histogram)",
      "type": "timeseries"
